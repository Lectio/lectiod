{
  "data": {
    "config": {
      "storage": {
        "type": "FILE_SYSTEM",
        "filesys": {
          "basePath": "./tmp/diskv_data"
        }
      },
      "harvest": {
        "ignoreURLsRegExprs": [
          "^https://twitter.com/(.*?)/status/(.*)$",
          "https://t.co"
        ],
        "removeParamsFromURLsRegEx": [
          "^utm_"
        ],
        "followHTMLRedirects": true
      },
      "errors": []
    }
  }
}